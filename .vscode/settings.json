{
    "[typeup]": {
        "files.associations": {
            "*.tup": "typeup"
        },
        "editor.rulers": [100],
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.wordWrap": "on",
        "files.trimTrailingWhitespace": true,
        "files.insertFinalNewline": true
    },
    "typeup.validate.enable": true,
    "typeup.schema": "./templates/document/schema.json",
    "typeup.style.guide": "./STYLE_GUIDE.md",
    "typeup.snippets": "./templates/snippets.json",
    "typeup.template": "./templates/document/template.tup",
    "typeup.cheatsheet": "./templates/document/CHEAT_SHEET.md"
}
