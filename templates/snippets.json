{
  "Document Header": {
    "prefix": "tup-header",
    "body": [
      "template = ${1:http://typeup.cogneco.com/templates/document}",
      "class = ${2:document}",
      "title = ${3:Document Title}",
      "date = ${4:YYYY-MM-DD}",
      "author = ${5:Author Name}",
      "",
      "$0"
    ],
    "description": "Basic document header metadata"
  },
  "Code Block": {
    "prefix": "tup-code",
    "body": [
      "%% ${1:language}",
      "${2:code}",
      "%%",
      "${3:caption}",
      ""
    ],
    "description": "Code block with optional caption"
  },
  "Math Block": {
    "prefix": "tup-math",
    "body": [
      "$$${1:formula}$$",
      "${2:caption}",
      ""
    ],
    "description": "Math block with optional caption"
  },
  "Table": {
    "prefix": "tup-table",
    "body": [
      "| ${1:Header 1} | ${2:Header 2} |",
      "|${3:---------}|${4:----------:}|",
      "| ${5:Cell 1}  | ${6:Cell 2}   |",
      "${7:caption}",
      ""
    ],
    "description": "Table with optional caption"
  },
  "Figure": {
    "prefix": "tup-figure",
    "body": [
      "!figure ${1:path/to/image}",
      "${2:caption}",
      ""
    ],
    "description": "Figure with optional caption"
  },
  "Video": {
    "prefix": "tup-video",
    "body": [
      "!video ${1:url}",
      "${2:caption}",
      ""
    ],
    "description": "Video with optional caption"
  },
  "Definition List": {
    "prefix": "tup-deflist",
    "body": [
      "${1:Term}",
      ": ${2:Definition 1}",
      ": ${3:Definition 2}",
      ""
    ],
    "description": "Definition list entry"
  },
  "Chapter": {
    "prefix": "tup-chapter",
    "body": [
      "===",
      "# ${1:Chapter Title}",
      "${2:content}",
      ""
    ],
    "description": "New chapter"
  },
  "Section": {
    "prefix": "tup-section",
    "body": [
      "---",
      "${1:content}",
      ""
    ],
    "description": "New section"
  },
  "Import": {
    "prefix": "tup-import",
    "body": [
      "!import ${1:filename}",
      ""
    ],
    "description": "Import another document"
  }
}
